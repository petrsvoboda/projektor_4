{% extends 'layout.twig' %}

{% block page_title %}
    Profile
{% endblock %}

{% block content %}
    <p><a href="{{ urlList }}">Back to list of profiles</a></p>   
    {% if urlEdit %}
        <p><a href="{{ urlEdit }}">Profile edit</a></p>
    {% endif %}
    {% if creatorName %}
        <p>Profile created by {{ creatorName }}</p>
    {% endif %}        
    {% include 'form.twig' %}
{% endblock %}
      
     